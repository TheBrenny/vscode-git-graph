{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}",
				"--extensions-dir=${workspaceFolder}/.vscode-profile/extensions/",
				"--user-data-dir=${workspaceFolder}/.vscode-profile/profile/"
			],
			"outFiles": [
				"${workspaceFolder}/out/**/*.js"
			]
		}
	]
}